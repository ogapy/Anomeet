<h1>MeetingRooms#index</h1>
<p>Find me in app/views/meeting_rooms/index.html.erb</p>

<% @attending_meeting_rooms.each do |attending_meeting_room| %>
  <div>
    <%= attending_meeting_room.name %>
    <%= attending_meeting_room.leader_id %>
    <%= attending_meeting_room.meeting_id %>
  </div>
<% end %>

<%= form_with model: @new_meeting_room, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.submit "ルーム作成"%>
<% end %>